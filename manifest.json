{
    "manifest_version": 3,
    "name": "100Devs Social Tracker",
    "version": "1.0",
    "description": "a social layer for communitytaught.org",
    "content_scripts": [
    {
        "matches": ["https://communitytaught.org/*"],
        "js": ["node_modules/appwrite/dist/esm/sdk.js", "jquery-3.1.1.min.js", "./main.js"]
    }
    ],
    "action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    }
  }