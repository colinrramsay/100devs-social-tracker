{
  "manifest_version": 3,
  "name": "100Devs Social Overlay",
  "version": "1.0",
  "description": "a social layer for communitytaught.org",
  "content_scripts": [
    {
      "matches": ["https://communitytaught.org/*"],
      "js": [
        "./main.js"
      ],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"]
}
